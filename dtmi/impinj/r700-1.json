                {
                  "name": "idle",
                  "enumValue": "idle",
                  "displayName": {
                    "en": "Idle"
                  }
                },
                {
                  "name": "enabled",
                  "enumValue": "enabled",
                  "displayName": {
                    "en": "Enabled"
                  }
                },
                {
                  "name": "active",
                  "enumValue": "active",
                  "displayName": {
                    "en": "Active"
                  }
                }
              ]
            }
          },
          {
            "name": "llrpConnection",
