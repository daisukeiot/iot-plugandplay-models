        "en": "Test Property String"
      },
      "name": "testPropertyString",
      "schema": "string",
      "writable": true
    },
    {
      "@id": "dtmi:impinj:R700:stopPreset;1",
      "@type": "Command",
      "commandType": "synchronous",
      "displayName": {
        "en": "Stop Preset"
      },
      "name": "stopPreset"
    },
    {
      "@id": "dtmi:impinj:R700:startPreset;1",
      "@type": "Command",
      "commandType": "synchronous",
      "displayName": {
        "en": "Start Preset"
      },
      "name": "startPreset",
      "request": {
        "@type": "CommandPayload",
        "displayName": {
          "en": "Preset Name"
