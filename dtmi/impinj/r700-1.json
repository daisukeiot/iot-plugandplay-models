            },
            "description": {
              "en": "Contains configuration options for publishing data to HTTP streams. These settings apply to all HTTP streams and also affect the setting of the history buffer that will be used when a new HTTP stream is established."
            },
            "schema": "string"
          },
          {
            "name": "eventPerSecondLimit",
            "displayName": {
              "en": "Manufacturer"
            },
            "description": {
              "en": "The reader manufacturer."
            },
            "schema": "string"
          },
          {
            "name": "eventAgeLimitMinutes",
            "displayName": {
